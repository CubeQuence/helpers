{
    "name": "cubequence/helpers",
    "description": "Helpers for CubeQuence",
    "keywords": [
        "helpers",
        "cubequence"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Castelnuovo",
            "email": "luca@castelnuovo.xyz"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "cubequence/crypto": "^1.1",
        "cubequence/oauth": "^1.1",
        "cubequence/request": "^1.0",
        "laminas/laminas-diactoros": "^2.5",
        "ramsey/uuid": "^4.1",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "dev-master",
        "pestphp/pest": "^1.0"
    },
    "scripts": {
        "tests": "./vendor/bin/pest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CQ\\Helpers\\": "src/Helpers"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/CubeQuence/helpers/issues",
        "source": "https://github.com/CubeQuence/helpers"
    }
}
